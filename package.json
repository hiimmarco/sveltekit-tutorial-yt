{
	"name": "sveltekit-tutorial-yt",
	"version": "0.0.1",
	"scripts": {
		"dev:only": "svelte-kit dev",
		"build:only": "svelte-kit build",
		"tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development  postcss src/styles/tailwind.css -o static/app.css -w",
		"tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production  postcss src/styles/tailwind.css -o static/app.css",
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-cloudflare": "^1.0.0-next.6",
		"@sveltejs/adapter-static": "^1.0.0-next.24",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss": "^8.4.5",
		"postcss-cli": "^9.1.0",
		"postcss-import": "^14.0.2",
		"postcss-nesting": "^10.1.2",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.44.0",
		"tailwindcss": "^3.0.15"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^14.2.0"
	}
}
